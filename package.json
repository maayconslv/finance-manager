{
  "name": "financy-manager",
  "version": "1.0.0",
  "description": "API de gerenciamento financeiro com Clean Architecture",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register 'src/**/*.test.ts'",
    "test:watch": "mocha -r ts-node/register -r tsconfig-paths/register --watch 'src/**/*.test.ts'",
    "test:e2e": "mocha -r ts-node/register -r tsconfig-paths/register 'src/**/*.e2e.test.ts'",
    "test:e2e:watch": "mocha -r ts-node/register -r tsconfig-paths/register --watch 'src/**/*.e2e.test.ts'",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:test:generate": "prisma generate --schema=./prisma/schema.test.prisma",
    "prisma:test:migrate": "prisma migrate dev --schema=./prisma/schema.test.prisma",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "express",
    "typescript",
    "clean-architecture",
    "prisma"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "prisma": "^5.7.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.4",
    "typedi": "^0.10.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "chai": "^4.3.10",
    "eslint": "^8.56.0",
    "mocha": "^10.2.0",
    "supertest": "^7.1.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
